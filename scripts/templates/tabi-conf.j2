[program:tabi]
directory="{{ ansible_user_dir }}/tabi"
environment=HOME="{{ ansible_user_dir }}"
user={{ ansible_user_id }}
command=/usr/bin/python examples/annotation/detect_hijacks.py -c local -i mabo --registry-path registry --bgp-path bgp
autostart=true
autorestart=true
stderr_logfile=/var/log/long.err.log
stdout_logfile=/var/log/long.out.log
verbose = True
